pipeline {
    agent any
    stages {
        stage('build')
        {
            steps {
                sh 'mvn install'
            }
        }
        stage('integrate') {
            steps {
                sh 'mvn clean'
            }
        }
        stage('test')
        {
            steps {
                sh 'mvn test'
            }
        }
        stage('release')
        {
            steps {
                sh 'mvn --version '
            }
        }
    }
}