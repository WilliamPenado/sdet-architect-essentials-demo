pipeline {
    agent any
    stages {
        stage('build')
        {
            steps {
                bat 'mvn clean'
                bat 'mvn install'
        }
        stage('integrate') {
            steps {
                bat 'mvn --version'
            }
        }
        stage('test')
        {
            steps {
                bat 'mvn test'
            }
        }
        stage('release')
        {
            steps {
                bat 'mvn --version '
            }
        }
    }
}